<table></table>
    <thead>
        <tr>
            <th> Patient ID </th>
            <th> First Name </th>
            <th> Last Name </th>
            <th> Date of Birth </th>
            <th> Gender </th>
            <th> Language </th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row =>{%>
            <tr>
                <td> <%= row.patient_id %> </td>
                <td> <%= row.first_name %> </td>
                <td> <%= row.last_name %> </td>
                <td> <%= row.date_of_birth %> </td>
                <td> <%= row.gender %> </td>
                <td> <%= row.language %> </td>
            </tr>
        <%});%>
    </tbody>
</table>